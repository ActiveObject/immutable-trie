  "keywords": [
    "immutable",
    "persistent",
    "trie",
    "hamt"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ActiveObject/immutable-trie.git"
  },
  "author": "Vasyl Pryimachuk <activeobject.ac@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "~1.9.0",
    "istanbul": "^0.3.0",
    "mocha": "~1.17.1"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "babel -d lib/ src/",
